{
  "cleanUrls": true,
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/icon.png",
      "destination": "/icon.png"
    },
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}